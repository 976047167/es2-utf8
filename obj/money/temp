// thousand.c

inherit MONEY;

void create()
{
	set_name("ㄢ蝗布", ({"thousand-cash", "thousand-cash_money", "cash"}));
	if( clonep() )
		set_default_object(__FILE__);
	else {
		set("money_id", "thousand-cash");
		set("long", "眎肂ㄢ蝗蝗布\n");
		set("unit", "舼");
		set("base_value", 100000);
		set("base_unit", "眎");
		set("base_weight", 3);
	}
	set_amount(1);
}

